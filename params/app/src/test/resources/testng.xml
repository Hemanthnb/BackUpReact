<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parameterization Suite">
    <test name="Parameterization Test">
        <parameter name="num1" value="10" />
        <parameter name="num2" value="20" />
        <classes>
            <class name="org.example.MyTestClass" />
        </classes>
    </test>

    <test name="AppTest">
        <parameter name="mobile" value="Android"></parameter>
        <classes>
            <class name="org.example.AppTest"/>
        </classes>
    </test>
</suite>
